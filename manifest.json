{
  "name": "GitGPT",
  "description": "A plugin that allows ChatGPT to create repos and push code to your GitHub.",
  "version": "0.1",
  "functions": [
    {
      "name": "createRepo",
      "route": "/createRepo",
      "httpMethod": "POST",
      "requiredParameters": ["githubPAT"]
    },
    {
      "name": "editFile",
      "route": "/editFile",
      "httpMethod": "POST",
      "requiredParameters": ["githubPAT"]
    },
    {
      "name": "pushCode",
      "route": "/pushCode",
      "httpMethod": "POST",
      "requiredParameters": ["githubPAT"]
    }
  ]
}
